{
    "name": "webeweb/haveibeenpwned-bundle",
    "type": "symfony-bundle",
    "description": "Integrate HaveIBeenPwned API with Symfony 2 and more",
    "keywords": [
        "API",
        "bundle",
        "haveibeenpwned",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/haveibeenpwned-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "webeweb/core-bundle": "^2.0",
        "webeweb/haveibeenpwned-library": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|^7.0"
    },
    "support": {
        "issues": "https://github.com/webeweb/haveibeenpwned-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\HaveIBeenPwnedBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\HaveIBeenPwnedBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
